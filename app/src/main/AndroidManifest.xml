<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unicorn.qingkee" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.main.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.main.ModifyServerAddressActivity"/>
        <activity android:name=".activity.main.SelectActivity"/>
        <activity android:name=".activity.main.MainActivity"/>
        <activity android:name=".activity.asset.ArrivalAssetListActivity"/>
        <activity android:name=".activity.asset.AssetBindActivity"/>
        <activity android:name=".activity.asset.AssetsListActivity"/>

    </application>

</manifest>
